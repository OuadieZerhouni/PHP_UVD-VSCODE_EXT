{
  "name": "php-uvd",
  "displayName": "PHP UVD",
  "publisher": "OuadieZerhouni",
  "description": "php unused variables detector",
  "version": "0.0.3",
  "engines": {
    "vscode": "^1.86.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:php-uvd.analyzeFile",
    "onLanguage:php"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "php-uvd.analyzeFile",
        "title": "Analyze PHP File"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.86.0",
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "eslint": "^8.56.0",
    "typescript": "^5.3.3",
    "@vscode/test-cli": "^0.0.4",
    "@vscode/test-electron": "^2.3.9"
  }
}